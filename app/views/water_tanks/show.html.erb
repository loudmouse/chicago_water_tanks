<div class="page-header">
  <%= link_to water_tanks_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_water_tank_path(@water_tank), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show water_tank</h1>
</div>

<dl class="dl-horizontal">
  <dt>Latitude:</dt>
  <dd><%= @water_tank.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @water_tank.longitude %></dd>

  <dt>Nickname:</dt>
  <dd><%= @water_tank.nickname %></dd>

  <dt>Address:</dt>
  <dd><%= @water_tank.address %></dd>

  <dt>Neighborhood:</dt>
  <dd><%= @water_tank.neighborhood %></dd>

</dl>
